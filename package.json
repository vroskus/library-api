{
  "name": "@vroskus/library-api",
  "version": "1.0.16",
  "description": "Api",
  "author": "Vilius Ro≈°kus <vilius@regattas.eu>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vroskus/library-api.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npm run test:eslint && npm run test:e2e",
    "test:eslint": "ESLINT_USE_FLAT_CONFIG=false eslint src test --fix",
    "test:e2e": "npm run test:e2e:run && npm run test:e2e:post",
    "test:e2e:run": "jest --ci --colors",
    "test:e2e:post": "jest-coverage-thresholds-bumper"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "axios-mock-adapter": "^1.22.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.1",
    "@types/node": "^20.12.10",
    "@types/query-string": "^6.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-problems": "^8.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-perfectionist": "^2.10.0",
    "eslint-plugin-react": "^7.34.1",
    "jest": "^29.7.0",
    "jest-coverage-thresholds-bumper": "^1.1.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 52.77,
        "functions": 48.27,
        "lines": 67.85,
        "statements": 65.26
      }
    }
  }
}
